<odoo>
    <record id="ir_cron_generate_appointment_dates" model="ir.cron">
        <field name="name">Generate Appointment Dates</field>
        <field name="model_id" ref="model_hospital_appointment_date"/>
        <field name="state">code</field>
        <field name="code">model._generate_appointment_dates()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>

        <field name="active" eval="True"/>
    </record>
</odoo>
